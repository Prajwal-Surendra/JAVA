

import java.util.*;

class Case
{
	public static void main (String args[]) 
	{
		int ch;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the choice\n 1.Hi \t 2.Hey \t 3.Hello");
		ch=sc.nextInt();
		switch (ch)
	 { 
	 	case 1: System.out.println("1.You said Hi");
	 	break; 
	 	case 2: System.out.println("2.You said Hey");
	 	break;
	 	case 3: System.out.println("1.You said Hello");	
	 	break;
	 	
	 	default: System.out.println("Invalid choice"); 	
		}
	}
}
